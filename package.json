{
  "name": "edl-design",
  "version": "1.0.0",
  "description": "Website for digital design studio, EDL",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "serve": "live-server ./build",
    "watch": "tailwindcss -i ./src/css/index.css -o ./build/css/styles.css --watch",
    "build": "NODE_ENV=production tailwindcss -i ./src/css/index.css -o ./build/css/styles.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frederickandersen/edl-design.git"
  },
  "author": "Frederick Andersen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frederickandersen/edl-design/issues"
  },
  "homepage": "https://github.com/frederickandersen/edl-design#readme",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.1.0",
    "live-server": "^1.1.0",
    "postcss": "^8.4.27",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "netlify-plugin-cloudinary": "1.17.0"
  }
}
